--exclude=.git
--exclude=.svn
--exclude=*.min.js
--exclude=jquery*.js
--exclude=jrails.js

--regex-ruby=/\s*(class|module)\s+[a-zA-Z0-9_:]+:([a-zA-Z0-9_]+)/\2/
--regex-ruby=/^\s*named_scope\s*:([a-zA-Z0-9_]+)/\1/
--regex-ruby=/^\s*scope\s*:([a-zA-Z0-9_]+)/\1/
--regex-ruby=/^\s*def_erb_method\s*\(?[\s\r\n]*["']([a-zA-Z0-9_]+)/\1/

--regex-javascript=/(,|(;|^)[ \t]*(var|let|([A-Za-z_$][A-Za-z0-9_$.]+\.)*))[ \t]*([A-Za-z0-9_$]+)[ \t]*=[ \t]*function[ \t]*\(/\5/,function/
--regex-javascript=/(,|^)[ \t]*([A-Za-z_$][A-Za-z0-9_$]+)[ \t]*:[ \t]*function[ \t]*\(/\2/,function/
